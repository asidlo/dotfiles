                "colorScheme": "Tokyonight",
                "commandline": "C:/Program Files/Git/bin/bash.exe",
                "guid": "{05f867ca-3efc-499c-81d1-fc719183133f}",
                "icon": "C:/Program Files/Git/mingw64/share/git/git-for-windows.ico",
                "name": "Git Bash",
                "startingDirectory": "%USERPROFILE%"
            },
            {
                "commandline": "pwsh -noe -c \"$vsPath = &(Join-Path 'C:\\Program Files (x86)' '\\Microsoft Visual Studio\\Installer\\vswhere.exe') -property installationpath -latest; Import-Module (Join-Path $vsPath 'Common7\\Tools\\Microsoft.VisualStudio.DevShell.dll'); Enter-VsDevShell -VsInstallPath $vsPath -SkipAutomaticLocation\"",
                "guid": "{78b73317-748b-5da1-a0e7-7a24d03cfbf2}",
                "hidden": false,
                "icon": "%USERPROFILE%\\Pictures\\icons\\vs2022.png",
